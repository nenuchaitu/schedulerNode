#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/my_apps/schedulerNode/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/dist/bin/uuid/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/dist/bin/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/dist/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/uuid@8.3.2/node_modules/uuid/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/uuid@8.3.2/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/node_modules:/home/theia/my_apps/schedulerNode/node_modules:/home/theia/my_apps/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/my_apps/schedulerNode/node_modules/uuid/dist/bin/uuid/node_modules:/home/theia/my_apps/schedulerNode/node_modules/uuid/dist/bin/node_modules:/home/theia/my_apps/schedulerNode/node_modules/uuid/dist/node_modules:/home/theia/my_apps/schedulerNode/node_modules/uuid/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/dist/bin/uuid" "$@"
else 
  exec node  "$basedir/../uuid/dist/bin/uuid" "$@"
fi
