#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/my_apps/schedulerNode/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/semver.js/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/semver@6.3.0/node_modules/semver/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/semver@6.3.0/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/node_modules:/home/theia/my_apps/schedulerNode/node_modules:/home/theia/my_apps/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/package-json@6.5.0/node_modules/package-json/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/package-json@6.5.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@6.3.0/node_modules/semver/bin/semver.js" "$@"
else 
  exec node  "$basedir/../../../../../semver@6.3.0/node_modules/semver/bin/semver.js" "$@"
fi
