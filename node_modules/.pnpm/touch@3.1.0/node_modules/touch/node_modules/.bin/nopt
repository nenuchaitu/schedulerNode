#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/theia/my_apps/schedulerNode/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/nopt@1.0.10/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/node_modules:/home/theia/my_apps/schedulerNode/node_modules:/home/theia/my_apps/node_modules:/home/theia/node_modules:/home/node_modules:/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/home/theia/my_apps/schedulerNode/node_modules/.pnpm/touch@3.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
fi
